{
  "name": "vuejs-smart-table",
  "version": "1.0.0-beta.6",
  "files": [
    "dist"
  ],
  "main": "./dist/vuejs-smart-table.umd.js",
  "module": "./dist/vuejs-smart-table.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vuejs-smart-table.es.js",
      "require": "./dist/vuejs-smart-table.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && yarn run tsDeclaration",
    "tsDeclaration": "tsc -d --outDir dist/types --emitDeclarationOnly --target esnext --moduleResolution node src/index.ts",
    "serve": "vite preview",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress serve docs"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "postcss-loader": "^5.2.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue": "^3.0.5",
    "vue-dts-gen": "^0.2.3",
    "vue-tsc": "^0.0.19",
    "vuepress": "^2.0.0-beta.8"
  },
  "dependencies": {
    "vue-demi": "^0.7.4"
  }
}
